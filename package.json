{
  "name": "check.js",
  "version": "0.1.0",
  "description": "Javascript error checking library",
  "main": "check.js",
  "scripts": {
    "size": "size-limit",
    "test": "jest --config config/jest.config.json --coverage",
    "test:watch": "jest --config config/jest.config.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://B-Core@bitbucket.org/B-Core/check.git"
  },
  "keywords": [
    "javascript",
    "error"
  ],
  "sideEffects": false,
  "author": "Ciro DE CARO",
  "license": "Apache-2.0",
  "homepage": "https://bitbucket.org/B-Core/check#readme",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cross-env": "^5.1.5",
    "jest": "^22.4.4",
    "size-limit": "^0.18.1"
  },
  "size-limit": [
    {
      "path": "check.js",
      "limit": "5 KB"
    }
  ],
  "dependencies": {
    "esm": "^3.0.38"
  }
}
